<p id="notice"><%= notice %></p>

<h1>Addvs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Image</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addvs.each do |addv| %>
      <tr>
        <td><%= addv.title %></td>
        <td><%= addv.image %></td>
        <td><%= addv.organization %></td>
        <td><%= link_to 'Show', addv %></td>
        <td><%= link_to 'Edit', edit_addv_path(addv) %></td>
        <td><%= link_to 'Destroy', addv, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Addv', new_addv_path %>
